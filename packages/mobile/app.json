{
  "expo": {
    "name": "Live Notes",
    "slug": "livestore-expo-deployed",
    "owner": "expo-production",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "livestore-expo",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.livestore.livestore-expodevelopment",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.livestore.livestoreexpo"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "60ec0edb-3993-4ef1-acc6-ef6d593ceed0"
      }
    },
    "plugins": [
      "expo-asset",
      "expo-sqlite",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
